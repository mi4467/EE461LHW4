        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="match_parent"-->
        <!--tools:context=".MapsActivity" />-->

    <com.google.android.gms.maps.MapView
        android:id="@+id/soleViewMap"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>


</LinearLayout>