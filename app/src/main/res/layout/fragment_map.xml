<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_height="match_parent"
    android:layout_width="match_parent">


    <EditText
        android:id="@+id/location_text_box_maps_location_fragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Enter A Location"
        />

    <Button
        android:id="@+id/enter_location_button_maps_location_fragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Enter Location"
        />

    <TextView
        android:id="@+id/first_extra_credit_location_fragment"
        android:text="Location Info"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:text="Wikepedia Article"
        android:id="@+id/location_web_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />

