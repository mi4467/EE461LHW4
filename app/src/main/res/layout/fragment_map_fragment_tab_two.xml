<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_height="match_parent"
    android:layout_width="match_parent">

    <EditText
        android:id="@+id/latitude_coordinate_fragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Enter Latitude"
        />

    <EditText
        android:id="@+id/longitude_coordinate_fragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Enter Longitude"
        />

    <Button
        android:id="@+id/enter_location_button_maps_coordinate_fragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Enter Coordinates"
        />

    <TextView
        android:id="@+id/first_extra_credit_coordinate_fragment"
        android:text="Location Info"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:text="Wikepedia Article"
        android:id="@+id/coordinate_web_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />

    <!--<fragment xmlns:android="http://schemas.android.com/apk/res/android"-->
